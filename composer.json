{
    "name":"appserver-io-apps/example",
    "description":"The example application for the ApplicationServer",
    "version":"1.0.0",
    "require":{
        "components/jquery":"*",
        "components/font-awesome":"*",
        "components/bootstrap":"3.3.4",
        "appserver-io/routlt":"~1.0",
        "jms/serializer" : "0.16.*"
    },
    "require-dev":{
        "appserver-io/phar":"~1.0",
        "appserver-io/build":"~1.0",
        "appserver-io/messaging":"~1.0",
        "appserver-io/lang":"~1.0",
        "appserver-io-psr/mop":"~1.0",
        "appserver-io-psr/epb":"~1.0",
        "appserver-io-psr/servlet":"~1.0",
        "doctrine/common":"~2.2"
    },
    "config":{
        "vendor-dir":"src/vendor",
        "component-dir":"src/components"
    },
    "autoload":{
        "psr-0":{
            "AppserverIo\\Apps\\Example":[
                "common/classes/",
                "src/WEB-INF/classes/",
                "src/META-INF/classes/",
                "tests/WEB-INF/classes/",
                "tests/META-INF/classes/"
            ]
            
        }
        
    },
    "authors":[
        {
            "name":"Tim Wagner",
            "email":"tw@appserver.io"
        },
        {
            "name":"Johann Zelger",
            "email":"jz@appserver.io"
        },
        {
            "name":"Bernhard Wick",
            "email":"bw@appserver.io"
        }
        
    ],
    "scripts":{
        "post-dump-autoload":[
            "ComponentInstaller\\Installer::postAutoloadDump"
        ]
    }
}