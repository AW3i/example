{
	"name" : "appserver-io-apps/example",
	"description" : "The example application for the ApplicationServer",
	"version" : "0.9.0",
	"require" : {
		"components/jquery" : "*",
		"components/font-awesome" : "*",
		"components/bootstrap" : "3.1.1",
		"appserver-io/routlt" : "0.3.*"
	},
	"require-dev" : {
		"appserver-io/phar" : "0.1.*",
		"appserver-io/build" : "0.2.*",
		"appserver-io/messaging" : "0.4.*",
		"appserver-io/lang" : "0.1.*",
		"appserver-io/doppelgaenger" : "0.5.*",
		"appserver-io-psr/epb" : "0.4.*",
		"appserver-io-psr/servlet" : "0.8.*",
		"doctrine/common" : "~2.2"
	},
	"config" : {
		"vendor-dir" : "src/vendor",
		"component-dir" : "src/components"
	},
	"autoload" : {
		"psr-0" : {
			"AppserverIo\\Apps\\Example" : [
				"common/classes/",
				"src/WEB-INF/classes/",
				"src/META-INF/classes/",
				"tests/WEB-INF/classes/",
				"tests/META-INF/classes/"
			]
		}
	},
	"authors" : [{
			"name" : "Tim Wagner",
			"email" : "tw@appserver.io"
		}, {
			"name" : "Johann Zelger",
			"email" : "jz@appserver.io"
		}, {
			"name" : "Bernhard Wick",
			"email" : "bw@appserver.io"
		}
	]
}